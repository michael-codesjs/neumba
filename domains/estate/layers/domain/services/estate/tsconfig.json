{
  "compilerOptions": {
    "lib": ["ES2020"],
    "target": "ES2020",
    "outDir": "bin",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true,
  },
  "include": [
    "**/*.ts",
    "**/*.js",
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
  ],
  "typeAcquisition": {
    "include": ["jest"]
  },
  "ts-node": {
    
  },
  "typedocOptions": {
    "mode": "modules",
    "out": "./docs"
  }
}